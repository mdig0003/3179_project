{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 300,
    "layer": [
      {
        "data": {
          "url": "data/choropleth map/suburb-10-vic (1).json",
          "format": {
            "type": "topojson",
            "feature": "suburb-10-vic"
          }
        },
        "projection": {
          "type": "mercator"
        },
        "mark": {
          "type": "geoshape",
          "fill": "lightgray",
          "stroke": "black",
          "strokeWidth":0.5
        }
      },
      {
        "data": {
          "url": "data/Melbourne_housing_FULL.csv"
        },
        "projection": {
          "type": "mercator"
        },
        "mark": "circle",
        "encoding": {
          "longitude": {
            "field": "longitude",
            "type": "quantitative"
          },
          "latitude": {
            "field": "latitude",
            "type": "quantitative"
          },
          "size": {"value": 10},
          "color": {"value": "steelblue"}
        }
      }
    ]
  }