{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 300,
  "title": "Melbourne Median Prices",
  "data": {
    "url": "data/choropleth map/suburb-10-vic (1).json",
    "format": {
      "type": "topojson",
      "feature": "suburb-10-vic"
    }
  },
  "transform": [
    {
      "lookup": "properties.vic_loca_2",
      "from": {
        "data": {
          "url": "data/choropleth map/map.csv"
        },
        "key": "SUBURB",
        "fields": ["Median House Price"]
      }
    }
  ],  
  "projection": {
    "type": "equirectangular"
  },  
  "mark": "geoshape",
  "encoding": {
    "color": {
      "field": "Median House Price",
      "title": "Median House Price ($AUS)",
      "type": "quantitative",
      "scale": {
        "range": ["#fdbe85", "#fd8d3c", "#e6550d", "#a63603"], 
        "domain": [0,2200000]
       }
    }
  }
}
