{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "height": 150,
    "width": 400,
    "data": {
      "url": "data/Line Chart/Scatterplot Region of Melbourne by Month.csv"
    },"transform": [
        {"fold": [  "January",
                    "February",
                    "March",
                    "April",
                    "May",
                    "June",
                    "July",
                    "August",
                    "September",
                    "October",
                    "November",
                    "December"
                ], "as": ["Month", "Value"]}
      ],
    "mark": "line",
    "encoding": {
      "x": {"field": "Month", "type": "nominal"},
      "y": {"field": "Value", "type": "quantitative"},
      "color": {"field": "Region of Melbourne", "type": "nominal"} 
    }
  }