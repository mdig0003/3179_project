{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Scatterplot of Housing Prices by Number of Bedrooms",
    "width": 200,
    "height": 150,
    "data": {"url": "data/Scatterplot data/Distance vs type of home.csv"},
    "encoding": {
      "x": {"field": "Distance", "type": "quantitative", "title": "Distance from CBD","scale": {"reverse":false}},
      "y": {"field": "h", "type": "quantitative", "title": "Avg Price of Home $"},
      "color": {"value": "blue"}
    },
    "layer": [
      {
        "mark": {"type": "square"},
        "encoding": {
          "y": {"field": "t", "type": "quantitative"},
          "color": {"value": "green"},
          "tooltip": {"field": "t","title":null}
        }
      },
      {
        "mark": {"type": "circle"},
        "encoding": {
          "y": {"field": "u", "type": "quantitative"},
          "color": {"value": "red"},
          "tooltip": {"field": "u","title":null}

        }
      },
      {
        "mark": {"type": "square"},
        "encoding": {
          "y": {"field": "h", "type": "quantitative"},
          "color": {"value": "blue"},
          "tooltip": {"field": "h","title":null}
        }
      }
    ]
  }
  