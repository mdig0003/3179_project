{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 300,
    "height": 150,
    "config": {"background":"transparent"},
    "data": {"url": "data/Scatterplot data/Number of Bedrooms by Avg Price.csv"},
    "encoding": {
      "x": {"field": "Number of Bedrooms", "type": "nominal", "title": "Number of Bedrooms","scale": {"reverse":false}},
      "y": {"field": "h", "type": "quantitative", "title": "Avg Price of Home ($AUS)"},
      "color": {"value": "blue"}
    },
    "layer": [
      {
        "mark": {"type": "square"},
        "encoding": {
          "y": {"field": "t", "type": "quantitative"},
          "color": {"value": "green"}
        }
      },
      {
        "mark": {"type": "circle"},
        "encoding": {
          "y": {"field": "u", "type": "quantitative"},
          "color": {"value": "red"}
        }
      },
      {
        "mark": {"type": "square"},
        "encoding": {
          "y": {"field": "h", "type": "quantitative"},
          "color": {"value": "blue"}
        }
      }
    ]
  }
  